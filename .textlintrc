{
  "plugins": [
    "jser-markdown"
  ],
  "filters": {
    "node-types": {
        "nodeTypes": ["Link"]
    }
  },
  "rules": {
    "proofdict": {
      "autoUpdate": true
    },
    "common-misspellings": true,
    "max-ten": {
      "max": 3
    },
    "no-doubled-joshi": {
      "min_interval": 1
    },
    "sentence-length": {
      "max": 300
    },
    "no-start-duplicated-conjunction": {
      "interval": 2
    },
    "no-mix-dearu-desumasu": true,
    "no-dropping-the-ra": true
  }
}
